---
import CardWrapper from "./CardWrapper.astro";

const { tag, title, description, image, url } = Astro.props;
---

<CardWrapper class="card py-10 px-12 h-full">
  <button class="rounded-full ...">{tag}</button>
  <h3 class="text-3xl">{title}</h3>
  <p class="text-2xl">
    {description}
  </p>
</CardWrapper>
